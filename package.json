{
    "name": "lcars-framework",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "framer-motion": "^7.6.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-scripts": "5.0.1",
        "sass": "^1.55.0",
        "semantic-release": "^19.0.5"
    },
    "babel": {
        "presets": [
            "@babel/preset-react"
        ]
    },
    "scripts": {
        "storybook": "start-storybook -p 6006 -s public",
        "build-storybook": "build-storybook -s public -o docs",
        "release": "standard-version && git push --follow-tags origin main",
        "build": "rm -rf dist && mkdir dist && babel ./src/**/ -d dist --copy-files",
        "commit": "cz",
        "create": "sh create.sh",
        "prepare": "husky install && chmod ug+x .husky/*"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ],
        "overrides": [
            {
                "files": [
                    "**/*.stories.*"
                ],
                "rules": {
                    "import/no-anonymous-default-export": "off"
                }
            }
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/cli": "^7.19.3",
        "@babel/preset-react": "^7.18.6",
        "@commitlint/cli": "^16.1.0",
        "@commitlint/config-conventional": "^16.0.0",
        "@storybook/addon-actions": "^6.5.13",
        "@storybook/addon-coverage": "^0.0.5",
        "@storybook/addon-docs": "^6.5.13",
        "@storybook/addon-essentials": "^6.5.13",
        "@storybook/addon-interactions": "^6.5.13",
        "@storybook/addon-links": "^6.5.13",
        "@storybook/addon-outline": "^6.5.13",
        "@storybook/addon-viewport": "^6.5.13",
        "@storybook/builder-webpack5": "^6.5.13",
        "@storybook/manager-webpack5": "^6.5.13",
        "@storybook/node-logger": "^6.5.13",
        "@storybook/preset-create-react-app": "^4.1.2",
        "@storybook/react": "^6.5.13",
        "@storybook/test-runner": "^0.9.0",
        "@storybook/testing-library": "^0.0.13",
        "@storybook/theming": "^6.5.13",
        "babel-plugin-named-exports-order": "^0.0.2",
        "commitizen": "^4.2.4",
        "cz-conventional-changelog": "^3.3.0",
        "husky": "^7.0.4",
        "prettier": "^2.7.1",
        "prop-types": "^15.8.1",
        "standard-version": "^9.3.2",
        "storybook-color-picker": "^3.0.2",
        "storybook-dark-mode": "^1.1.2",
        "webpack": "^5.74.0"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FRickReich/lcars-framework.git"
    },
    "keywords": [],
    "author": "F. Rick Reich",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/FRickReich/lcars-framework/issues"
    },
    "homepage": "https://github.com/FRickReich/lcars-framework#readme"
}
